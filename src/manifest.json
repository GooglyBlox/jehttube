{
    "manifest_version": 2,
    "name": "Jehttube",
    "version": "1.0",
    "description": "Changes YouTube thumbnails to videos from TurboJehtt",
    "permissions": ["https://www.youtube.com/*"],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png",
      "256": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  